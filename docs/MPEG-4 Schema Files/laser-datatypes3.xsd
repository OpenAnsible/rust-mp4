    <simpleType name="rotateType">
        <union memberTypes="typ:fixed16_8Type">
            <simpleType>
                <restriction base="string">
                    <enumeration value="auto"/>
                    <enumeration value="auto-reverse"/>
                </restriction>
            </simpleType>
        </union>

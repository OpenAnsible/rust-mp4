        <restriction base="string">
            <enumeration value="begin"/>
            <enumeration value="end"/>
        </restriction>
    </simpleType>

    <simpleType name="animFillType">
        <restriction base="string">
            <enumeration value="freeze"/>
            <enumeration value="remove"/>
        </restriction>
    </simpleType>

    <simpleType name="repeatCountType">
        <union memberTypes="typ:fixed16_8Type">
            <simpleType>
                <restriction base="string">
                    <enumeration value="indefinite"/>
                </restriction>
            </simpleType>
        </union>

            <enumeration value="y"/>
            <enumeration value="rx"/>
            <enumeration value="ry"/>
            <enumeration value="r"/>
            <enumeration value="transform"/>
            <enumeration value="size"/>
            <enumeration value="display-align"/>
            <enumeration value="color"/>
            <enumeration value="audio-level"/>
            <enumeration value="fill-rule"/>
            <enumeration value="font-style"/>
            <enumeration value="font-weight"/>
            <enumeration value="image-rendering"/>
            <enumeration value="line-increment"/>
            <enumeration value="pointer-events"/>
            <enumeration value="shape-rendering"/>
            <enumeration value="stop-color"/>
            <enumeration value="stop-opacity"/>
            <enumeration value="solid-color"/>
            <enumeration value="solid-opacity"/>

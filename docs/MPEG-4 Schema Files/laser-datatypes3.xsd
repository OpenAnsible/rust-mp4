<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:typ="urn:mpeg:mpeg4:LASeR:types:2005"
        targetNamespace="urn:mpeg:mpeg4:LASeR:types:2005"
        elementFormDefault="qualified">

    <annotation>
        <documentation>
            These are the datatypes used in the schemas of 14496-20/AMD1
        </documentation>
    </annotation>

    <simpleType name="AttributeNameType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="AttributeNameListType">
        <list itemType="typ:AttributeNameType"/>
    </simpleType>

    <simpleType name="paintType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="colorType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="fontFamilyType">
        <union memberTypes="typ:fontFaceType">
            <simpleType>
                <restriction base="string">
                    <enumeration value="inherit"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <simpleType name="attrExtensionType">
        <restriction base="string"/>
        <!-- the codec for type extensions -->
    </simpleType>

    <simpleType name="typeCodecType">
        <restriction base="string"/>
        <!-- the pseudo type for complex type codecs -->
    </simpleType>

    <simpleType name="colorExType">
        <restriction base="typ:attrExtensionType"/>
    </simpleType>

    <simpleType name="color">
        <restriction base="typ:typeCodecType"/>
    </simpleType>

    <simpleType name="ZeroToOneFloat">
        <restriction base="float">
            <minInclusive value="0"/>
            <maxInclusive value="1"/>
        </restriction>
    </simpleType>

    <simpleType name="opacityType">
        <restriction base="typ:ZeroToOneFloat"/>
    </simpleType>

    <simpleType name="audioLevelType">
        <restriction base="typ:ZeroToOneFloat"/>
    </simpleType>

    <simpleType name="offsetType">
        <restriction base="typ:ZeroToOneFloat"/>
    </simpleType>

    <simpleType name="fontFaceType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="fontStyleType">
        <restriction base="string">
            <enumeration value="normal"/>
            <enumeration value="italic"/>
            <enumeration value="oblique"/>
            <enumeration value="inherit"/>
        </restriction>
    </simpleType>

    <simpleType name="textDecorationType">
        <restriction base="string">
            <enumeration value="none"/>
            <enumeration value="underline"/>
            <enumeration value="inherit"/>
            <enumeration value="LASeR_OUTLINE"/>
            <enumeration value="LASeR_EMBOSS"/>
            <enumeration value="LASeR_ENGRAVE"/>
            <enumeration value="LASeR_LEFTDROPSHADOW"/>
            <enumeration value="LASeR_RIGHTDROPSHADOW"/>
        </restriction>
    </simpleType>

    <simpleType name="calcModeType">
        <restriction base="string">
            <enumeration value="discrete"/>
            <enumeration value="linear"/>
            <enumeration value="paced"/>
            <enumeration value="spline"/>
        </restriction>
    </simpleType>

    <simpleType name="visibilityType">
        <restriction base="string">
            <enumeration value="visible"/>
            <enumeration value="hidden"/>
            <enumeration value="inherit"/>
        </restriction>
    </simpleType>

    <simpleType name="overflowType">
        <restriction base="string">
            <enumeration value="visible"/>
        </restriction>
    </simpleType>

    <simpleType name="displayType">
        <restriction base="string">
            <enumeration value="inline"/>
            <enumeration value="none"/>
            <enumeration value="inherit"/>
            <enumeration value="block"/>
            <enumeration value="list-item"/>
            <enumeration value="run-in"/>
            <enumeration value="compact"/>
            <enumeration value="marker"/>
            <enumeration value="table"/>
            <enumeration value="inline-table"/>
            <enumeration value="table-row-group"/>
            <enumeration value="table-header-group"/>
            <enumeration value="table-footer-group"/>
            <enumeration value="table-row"/>
            <enumeration value="table-column-group"/>
            <enumeration value="table-column"/>
            <enumeration value="table-cell"/>
            <enumeration value="table-caption"/>
        </restriction>
    </simpleType>

    <simpleType name="textAnchorType">
        <restriction base="string">
            <enumeration value="start"/>
            <enumeration value="middle"/>
            <enumeration value="end"/>
            <enumeration value="inherit"/>
        </restriction>
    </simpleType>

    <simpleType name="eventType">
        <union memberTypes="string"> <!-- string added for private events/extensions -->
            <simpleType>
                <restriction base="string">
                    <enumeration value="focusin"/>
                    <enumeration value="focusout"/>
                    <enumeration value="activate"/>
                    <enumeration value="mousedown"/>
                    <enumeration value="mouseup"/>
                    <enumeration value="click"/>
                    <enumeration value="beginEvent"/>
                    <enumeration value="endEvent"/>
                    <enumeration value="repeatEvent"/>
                    <enumeration value="zoom"/>
                    <enumeration value="scroll"/>
                    <enumeration value="resize"/>
                    <enumeration value="error"/>
                    <enumeration value="abort"/>
                    <enumeration value="load"/>
                    <enumeration value="unload"/>
                    <enumeration value="keydown"/>
                    <enumeration value="keyup"/>
                    <enumeration value="accessKey"/>
                    <enumeration value="longAccessKey"/>
                    <enumeration value="textinput"/>
                    <enumeration value="mouseover"/>
                    <enumeration value="mouseout"/>
                    <enumeration value="mousemove"/>
                    <enumeration value="pause"/>
                    <enumeration value="resume"/>
                    <enumeration value="pausedEvent"/>
                    <enumeration value="resumedEvent"/>
                    <enumeration value="activatedEvent"/>
                    <enumeration value="deactivatedEvent"/>
                    <enumeration value="LASeRConnecting"/> <!-- AMD1 -->
                    <enumeration value="LASeRBuffering"/> <!-- AMD1 -->
                    <enumeration value="LASeREndStream"/> <!-- AMD1 -->
                    <enumeration value="LASeRPlayable"/> <!-- AMD1 -->
                    <enumeration value="executionTime"/> <!-- this value is not an event, but may appear
                    in the same place as an event in time expressions replaced or inserted into begin
                    and end attributes of timed elements -->
                    <enumeration value="screenOrientation0"/>
                    <enumeration value="screenOrientation90"/>
                    <enumeration value="screenOrientation180"/>
                    <enumeration value="screenOrientation270"/>
                    <enumeration value="screenOrientationPortrait"/>
                    <enumeration value="screenOrientationLandscape"/>
                    <enumeration value="activatedEvent"/>
                    <enumeration value="deactivatedEvent"/>
                    <enumeration value="DisplaySizeChanged"/> <!-- AMD2 -->
                    <enumeration value="MemoryStatus"/> <!-- AMD2 -->
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <simpleType name="fill-ruleType">
        <restriction base="string">
            <enumeration value="evenodd"/>
            <enumeration value="nonzero"/>
            <enumeration value="inherit"/>
        </restriction>
    </simpleType>

    <simpleType name="pointType">
        <restriction>
            <simpleType>
                <list itemType="typ:fixed16_8Type"/>
            </simpleType>
            <length value="2"/>
        </restriction>
    </simpleType>

    <simpleType name="viewBoxType">
        <restriction>
            <simpleType>
                <list itemType="typ:fixed16_8Type"/>
            </simpleType>
            <length value="4"/>
        </restriction>
    </simpleType>

    <simpleType name="transformType">
        <union memberTypes="typ:matrix typ:matrixExType typ:pointType"/>
    </simpleType>

    <simpleType name="matrix">
        <restriction>
            <simpleType>
                <list itemType="float"/>
            </simpleType>
            <length value="6"/>
        </restriction>
    </simpleType>

    <simpleType name="matrixExType">
        <restriction base="typ:attrExtensionType"/>
    </simpleType>


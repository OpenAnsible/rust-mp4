        <restriction base="string">
            <enumeration value="always"/>
            <enumeration value="whenNotActive"/>
            <enumeration value="never"/>
        </restriction>
    </simpleType>

    <simpleType name="rotscatra">
        <restriction base="string">
            <enumeration value="translate"/>
            <enumeration value="scale"/>
            <enumeration value="rotate"/>
            <enumeration value="skewX"/>
            <enumeration value="skewY"/>
        </restriction>
    </simpleType>

    <simpleType name="choiceType">
        <union>
            <simpleType>
                <restriction base="unsignedInt">
                    <maxInclusive value="255"/>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="string">
                    <enumeration value="none"/>
                    <enumeration value="all"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <simpleType name="simpleEncodingType">
        <restriction base="unsignedInt">
            <maxInclusive value="3"/>
        </restriction>
    </simpleType>

    <simpleType name="resolutionType">
        <restriction base="int">
            <minInclusive value="-7"/>
            <maxInclusive value="8"/>
        </restriction>
    </simpleType>

    <simpleType name="u5b">
        <restriction base="unsignedInt">
            <maxInclusive value="31"/>
        </restriction>
    </simpleType>

    <simpleType name="pathType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="timeType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="smilTimesType">
        <restriction base="string"/>
    </simpleType>

    <simpleType name="syncToleranceType">
        <union memberTypes="float">           <!-- should not be int but float -->
            <simpleType>
                <restriction base="string">
                    <enumeration value="default"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <simpleType name="syncToleranceDefaultType">
        <union memberTypes="float">               <!-- should not be int but float -->
            <simpleType>
                <restriction base="string">
                    <enumeration value="inherit"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <simpleType name="indexType">
        <restriction base="int">
            <minInclusive value="-1"/>
        </restriction>
    </simpleType>

